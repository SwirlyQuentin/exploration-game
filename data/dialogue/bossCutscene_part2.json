    "dialogue": [
        {
            "key": "will",
            "content": "Rat? What are you doing here?",
            "direction": "right"
        },
        {
            "key": "boss",
            "content": "Uncle can I have your Rat?",
            "direction": "center"
        },
        {
            "key": "will",
            "content": "For the hundredth time, you cannot have him,",
            "direction": "right"
        },
        {
            "key": "will",
            "content": "but you can always come visit,",
            "direction": "right"
        },
        {
            "key": "will",
            "content": "you know this",
            "direction": "right"
        },
        {
            "key": "boss",
            "content": "But I wannnntttt hiiiiimmm",
